@model Taskify.Models.ApplicationUser

@{
    ViewBag.Title = "Index";
}

<h2>Show users</h2>
<br />
@if (ViewBag.Message != null)
{
    <h2 class="alert-success p-3 rounded-3 text-center mb-5">@ViewBag.Message</h2>
}




<br />


@foreach (Taskify.Models.ApplicationUser user in ViewBag.Users)
{
    <div class="card">
         

        <div class="card-body">

            <h3 class="card-title alert-info py-3 px-3 rounded-2">@user.Email</h3>

            <div class="card-text">rol</div>
            
        </div>
       
    

    </div>

    <br />
    <br />
}



